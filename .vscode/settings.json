// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
  },
  "search.exclude": {
    "dist/": true,
    "**/node_modules": true,
  },
  "editor.tabSize": 2,
  "files.insertFinalNewline": true,
  "editor.insertSpaces": true,
  "files.trimTrailingWhitespace": true,
  "editor.detectIndentation": true,
  "editor.rulers": [100]
}
